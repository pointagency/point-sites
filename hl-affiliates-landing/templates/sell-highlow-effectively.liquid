								$('.stunning-creative--item').velocity('stop');
								$('.stunning-creative--item').removeAttr('style');
								$('.stunning-creative--item').addClass('in');
								$('#stunning-creative--arrows').velocity({
									opacity : 1
								});

								if(callback) {
									callback();
								}

