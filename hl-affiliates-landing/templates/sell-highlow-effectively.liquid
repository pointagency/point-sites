								topOffset = 5;

							setTimeout(function(){

								$sellEffectiveHeadingSell.velocity({
									top: $appropriateMessagingHeadingSell.offset().top+topOffset,
									left: $appropriateMessagingHeadingSell.offset().left,
									opacity: 1
								},{
									duration: 1750,
									easing: easing
								})

								$sellEffectiveHeadingHighlow.velocity({
									top: $appropriateMessagingHeadingHighlow.offset().top+topOffset,
									left: $appropriateMessagingHeadingHighlow.offset().left,
									opacity: 1
								},{
									duration: 1750,
									easing: easing
								})

								$sellEffectiveHeadingEffectively.velocity({
									top: $appropriateMessagingHeadingEffectively.offset().top+topOffset,
									left: $appropriateMessagingHeadingEffectively.offset().left,
									opacity: 1
								},{
									duration: 1750,
									easing: easing
								});

							},250);
							

							setTimeout(function(){
								
								execute();

							},2000);

						} else {
							execute();
						}

						currentStep = 'appropriate-message';
					}

					if(!slide5Loaded) {
						$slide5.imagesLoaded (function(){
							enter();
							slide5Loaded = true;
						});
					} else {
						enter();
					}
				},
				'stunning-creative' : function(callback) {
					
