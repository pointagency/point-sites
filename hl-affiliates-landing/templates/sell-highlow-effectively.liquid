						} else if (currentStep == 'intro') {

							$('#sell-highlow-effectively--text-content .unhighlighted-text').each(function(){
								var self = $(this);
								self.velocity({
									opacity: 0,
									top: ((self.offset().top+self.outerHeight()/2)<$(window).height()/2)?(Utils.getRandomInt(-120,0)+'px'):(Utils.getRandomInt(0,120)+'px'),
